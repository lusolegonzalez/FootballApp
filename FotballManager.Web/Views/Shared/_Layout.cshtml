<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>La Organización - @ViewData["Title"]</title>
    <link href="~/img/cua.ico" rel="shortcut icon" type="image/x-icon" />

    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/libs/bootstrap/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/libs/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/libs/ionicons/css/ionicons.min.css">

    @RenderSection("Styles", required: false)

    <!-- Theme style -->
    <link rel="stylesheet" href="~/libs/admin-lte/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
    folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/libs/admin-lte/css/skins/_all-skins.min.css">

    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">

</head>
<body class="hold-transition skin-grey-light ssidebar-mini fixed">

    <!-- Site wrapper -->
    <div class="wrapper">

        @await Html.PartialAsync("_Header")

        <!-- =============================================== -->
        <!-- Left side column. contains the sidebar -->

        @await Html.PartialAsync("_NavBar")

        @*@if (User.IsInRole("Administrator"))
            {
                @await Html.PartialAsync("_NavBarAdmin")
            }
            else if (User.IsInRole("Manager"))
            {
                @await Html.PartialAsync("_NavBarManager")
            }
            else if (User.IsInRole("Player"))
            {
                @await Html.PartialAsync("_NavBarSupervisor")
            }*@

        <!-- =============================================== -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">

            @await Html.PartialAsync("_StatusMessage", TempData["StatusMessage"])

            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        <!-- =============================================== -->
        <!-- Page footer. -->
        @await Html.PartialAsync("_Footer")

        <!-- =============================================== -->
        <!-- Control Sidebar -->
        @* @await Html.PartialAsync("_SideBar")*@
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="~/libs/jquery/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/libs/bootstrap/js/bootstrap.min.js"></script>

    @RenderSection("Scripts", required: false)

    <!-- SlimScroll -->
    <script src="~/libs/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/libs/fastclick/fastclick.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/libs/admin-lte/js/adminlte.min.js"></script>
    <!-- i18n -->
    <script src="~/libs/jquery.i18n/CLDRPluralRuleParser.js"></script>
    <script src="~/libs/jquery.i18n/jquery.i18n.js"></script>
    <script src="~/libs/jquery.i18n/jquery.i18n.messagestore.js"></script>
    <script src="~/libs/jquery.i18n/jquery.i18n.fallbacks.js"></script>
    <script src="~/libs/jquery.i18n/jquery.i18n.language.js"></script>
    <script src="~/libs/jquery.i18n/jquery.i18n.parser.js"></script>
    <script src="~/libs/jquery.i18n/jquery.i18n.emitter.js"></script>
    <script src="~/libs/jquery.i18n/jquery.i18n.emitter.bidi.js"></script>
    @*<script src="~/js/pages/main-jquery_i18n.js" asp-append-version="true"></script>*@
    <!-- Notifications -->
    @*<script src="~/libs/signalr/dist/browser/signalr.min.js"></script>
    <script src="~/js/notifications/notifications.js" asp-append-version="true"></script>*@

    <!-- ASP.NET site script -->
    <environment include="Development">
        <script src="~/js/site.js"></script>
        <script src="~/js/site-custom.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/site.min.js" asp-append-version="true"></script>
        <script src="~/js/site-custom.js"></script>
    </environment>

@await Html.PartialAsync("_ValidationScriptsPartial")
</body>
</html>
